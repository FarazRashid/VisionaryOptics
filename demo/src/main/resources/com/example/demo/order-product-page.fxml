<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.demo.OrderProductPageController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <ScrollPane fx:id="orderScrollPane" layoutX="293.0" layoutY="101.0" prefHeight="554.0" prefWidth="847.0">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="544.0" prefWidth="841.0">
                    <children>
                        <GridPane fx:id="orderGrid" layoutX="2.0" layoutY="8.0" prefHeight="544.0" prefWidth="756.0"
                                  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                  AnchorPane.topAnchor="0.0" hgap="10" vgap="10">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                        </GridPane>

                    </children>
                </AnchorPane>
            </content>
        </ScrollPane>
        <Label layoutX="293.0" layoutY="74.0" prefHeight="17.0" prefWidth="85.0" text="Id" />
        <Label layoutX="396.0" layoutY="74.0" text="Image" />
        <Label layoutX="488.0" layoutY="74.0" text="Name" />
        <Label layoutX="584.0" layoutY="74.0" text="Category" />
        <Label layoutX="676.0" layoutY="74.0" text="Quantity" />
        <Label layoutX="862.0" layoutY="74.0" text="Price" />
        <Label layoutX="819.0" layoutY="670.0" text="Total Price" />
        <Label fx:id="cartTotalAmount" layoutX="902.0" layoutY="670.0" text="{orderAmount}" />
        <Button fx:id="signOutButton" layoutX="20.0" layoutY="636.0" mnemonicParsing="false" onAction="#onClickSignOut" prefHeight="25.0" prefWidth="113.0" text="Sign Out" />
        <Button fx:id="viewProfileButton" layoutX="18.0" layoutY="594.0" mnemonicParsing="false" onAction="#onClickViewProfile" prefHeight="25.0" prefWidth="113.0" text="View Profile" />
        <Button fx:id="goBackHomeButton" layoutX="20.0" layoutY="553.0" mnemonicParsing="false" onAction="#onClickGoBackHome" prefHeight="25.0" prefWidth="113.0" text="Go Back To Home" />
        <Button fx:id="viewOrdersButton" layoutX="20.0" layoutY="511.0" mnemonicParsing="false" onAction="#onClickViewOrders" prefHeight="25.0" prefWidth="113.0" text="View Orders" />
    </children>

</AnchorPane>
